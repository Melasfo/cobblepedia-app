@model IEnumerable<ProjectTrial11.Models.TMMove>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TMNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MoveName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MoveType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MoveCategory)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MovePower)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MoveAccuracy)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TMNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MoveName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MoveType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MoveCategory)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MovePower)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MoveAccuracy)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
